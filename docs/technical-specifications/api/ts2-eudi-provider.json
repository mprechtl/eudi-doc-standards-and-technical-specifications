{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"id": "http://data.europa.eu/eudi/model",
	"type": "object",
	"properties":
	{
		"EUDI-Provider-2025-04-24-v2":
		{
			"type": "object",
			"anyOf":
			[
				{ "$ref": "#/definitions/Document" },
